{
  "name": "@type-dom/framework",
  "version": "0.0.9",
  "description": "一种基于抽象类和具体类的typescript的轻量级前端框架。A lightweight typescript front-end framework based on abstract classes, concrete classes. ",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "publish": "rimraf dist && tsc && copy README.md package.json dist && cd dist && npm publish",
    "test": "jest",
    "lint": "eslint src",
    "lint-fix": "eslint 'src/**/*.{ts,js,tsx,jsx}' --fix -o eslint_log.log",
    "uml": "tsuml2 --glob src/type-node/web-element/*.ts ",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "coverage": "typescript-coverage-report",
    "git-push": "git add . && git commit -m 'edit' && git push origin main",
    "git-pull": "git pull"
  },
  "keywords": [
    "type-dom",
    "type-dom.ts",
    "type-dom-ts",
    "front-end framework",
    "typescript",
    "TypeNode",
    "TypeDom",
    "Class based programming"
  ],
  "author": "xjf7711",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/type-dom/framework.git"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.2",
    "eslint": "^8.42.0",
    "eslint-webpack-plugin": "^4.0.1",
    "jest": "^29.6.2",
    "lint": "^1.1.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2",
    "typescript-coverage-report": "^1.0.0"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "@type-dom/utils": "^0.0.6"
  },
  "typeCoverage": {
    "atLeast": 99,
    "is": 99,
    "cache": true,
    "debug": true,
    "detail": true,
    "ignoreCatch": true,
    "ignoreFiles": [],
    "project": "tsconfig.json",
    "strict": true,
    "suppressError": true,
    "update": true,
    "ignoreUnread": true,
    "ignoreNested": true,
    "ignoreAsAssertion": true,
    "ignoreTypeAssertion": true,
    "ignoreNonNullAssertion": true,
    "showRelativePath": true,
    "historyFile": "typecoverage.json",
    "noDetailWhenFailed": true
  }
}
